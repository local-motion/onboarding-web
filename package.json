{
  "name": "onboarding-web",
  "version": "0.0.3",
  "private": true,
  "main": "dist/index.js",
  "homepage": "/",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom --passWithNoTests",
    "eject": "react-scripts eject",
    "lint:check": "npx eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "npx eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass-chokidar src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist"
  },
  "dependencies": {
    "@aws-amplify/ui": "1.0.15",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "apollo-cache-inmemory": "1.5.0",
    "apollo-client": "2.5.0",
    "apollo-link-context": "1.0.14",
    "apollo-link-error": "1.1.7",
    "apollo-link-http": "1.5.11",
    "ajv": "6.9.2",
    "autosuggest-highlight": "3.1.1",
    "aws-amplify": "1.1.19",
    "aws-amplify-react": "2.3.0",
    "classnames": "2.2.6",
    "cognito-user-pool": "0.2.4",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "history": "4.7.2",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next": "15.0.4",
    "lodash": "4.17.11",
    "lunr": "2.3.6",
    "moment": "2.24.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.5",
    "react": "16.8.3",
    "react-apollo": "2.5.0",
    "prop-types": "15.7.2",
    "react-autosuggest": "9.4.3",
    "react-cookie-consent": "2.2.1",
    "react-datepicker": "2.1.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.3",
    "react-google-maps": "9.4.5",
    "react-i18next": "10.2.0",
    "react-lunr": "0.5.0",
    "react-nouislider": "2.0.1",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "react-share": "2.4.0",
    "react-slick": "0.23.2",
    "react-swipeable-views": "0.13.1",
    "recompose": "0.30.0",
    "redux-thunk": "2.3.0",
    "apollo-link": "latest"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "9.0.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "prettier": "1.16.4",
    "redux": "4.0.1",
    "uuid": "3.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
