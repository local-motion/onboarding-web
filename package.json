{
  "name": "onboarding-web",
  "version": "0.0.3",
  "private": true,
  "main": "dist/index.js",
  "homepage": "/",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom --passWithNoTests",
    "eject": "react-scripts eject",
    "lint:check": "npx eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "npx eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass-chokidar src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist"
  },
  "dependencies": {
    "@aws-amplify/ui": "1.0.25",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "ajv": "6.10.2",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-context": "1.0.19",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "autosuggest-highlight": "3.1.1",
    "base": "3.0.0",
    "aws-amplify": "1.1.40",
    "aws-amplify-react": "2.4.4",
    "classnames": "2.2.6",
    "cognito-user-pool": "0.2.4",
    "graphql": "14.5.8",
    "date-fns": "2.4.1",
    "graphql-tag": "2.10.1",
    "history": "4.10.1",
    "i18next": "17.0.16",
    "lodash": "4.17.21",
    "i18next-browser-languagedetector": "3.0.3",
    "lunr": "2.3.6",
    "moment": "2.24.0",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "query-string": "6.8.3",
    "react-apollo": "3.1.1",
    "react": "16.10.1",
    "react-autosuggest": "9.4.3",
    "react-cookie-consent": "2.5.0",
    "react-datepicker": "2.9.6",
    "react-datetime": "2.16.3",
    "react-dom": "16.10.1",
    "react-google-maps": "9.4.5",
    "react-helmet": "5.2.1",
    "react-json-pretty": "2.1.0",
    "react-i18next": "10.13.1",
    "react-lunr": "0.6.0",
    "react-nouislider": "2.0.1",
    "react-places-autocomplete": "7.2.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.1",
    "react-scripts": "3.1.2",
    "react-share": "3.0.1",
    "react-slick": "0.25.2",
    "react-sticky-el": "1.0.20",
    "react-swipeable-views": "0.13.3",
    "recompose": "0.30.0",
    "redux-thunk": "2.3.0",
    "stringquery": "1.0.8"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-prettier": "3.1.1",
    "prettier": "1.18.2",
    "eslint-config-prettier": "6.3.0",
    "redux": "4.0.4",
    "uuid": "3.3.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
